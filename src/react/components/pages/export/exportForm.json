{
    "type": "object",
    "properties": {
        "assetState": {
            "type": "string",
            "title": "Asset State",
            "description": "Which assets to include in the export",
            "enum": [
                "all",
                "visited",
                "tagged"
            ],
            "default": "all",
            "enumNames": [
                "All Assets",
                "Only Visisted Assets",
                "Only Tagged Assets"
            ]
        },
        "providerType": {
            "title": "${strings.common.provider}",
            "type": "string",
            "enum": [
                "vottJson",
                "azureCustomVision",
                "tensorFlowRecords",
                "tensorFlowPascalVOC"
            ],
            "default": "vottJson",
            "enumNames": [
                "${strings.export.providers.vottJson}",
                "${strings.export.providers.azureCV}",
                "${strings.export.providers.tfRecords}",
                "${strings.export.providers.tfPascalVoc}"
            ]
        },
        "providerOptions": {
            "type": "object"
        }
    }
}